on init
    mount none /system/overlay/EmptyOverlay/EmptyOverlay.apk /vendor/overlay/CarrierConfigResCommon.apk bind
    mount none /system/overlay/EmptyOverlay/EmptyOverlay.apk /vendor/overlay/FrameworksResCommon.apk bind
    mount none /system/overlay/EmptyOverlay/EmptyOverlay.apk /vendor/overlay/FrameworksResTarget.apk bind
    mount none /system/overlay/EmptyOverlay/EmptyOverlay.apk /vendor/overlay/SystemUIResCommon.apk bind
    mount none /system/overlay/EmptyOverlay/EmptyOverlay.apk /vendor/overlay/TelecommResCommon.apk bind
    mount none /system/overlay/EmptyOverlay/EmptyOverlay.apk /vendor/overlay/TelephonyResCommon.apk bind
    mount none /system/overlay/EmptyOverlay/EmptyOverlay.apk /vendor/overlay/CellBroadcastReceiverResCommon.apk bind
    mount none /system/overlay/EmptyOverlay/EmptyOverlay.apk /vendor/overlay/DevicesAndroidOverlay.apk bind
    mount none /system/overlay/EmptyOverlay/EmptyOverlay.apk /vendor/overlay/ElderlyNavigationBarOverlay.apk bind
    mount none /system/overlay/EmptyOverlay/EmptyOverlay.apk /vendor/overlay/GestureLineOverlay.apk bind
    mount none /system/overlay/EmptyOverlay/EmptyOverlay.apk /vendor/overlay/MccMncOverlay.apk bind
    mount none /system/overlay/EmptyOverlay/EmptyOverlay.apk /vendor/overlay/MiuiBiometricResOverlay.apk bind
    mount none /system/overlay/EmptyOverlay/EmptyOverlay.apk /vendor/overlay/MiuiBluetoothOverlay.apk bind
    mount none /system/overlay/EmptyOverlay/EmptyOverlay.apk /vendor/overlay/MiuiFrameworkResOverlay.apk bind

service vendor.capabilityconfigstore /vendor/bin/hw/vendor.qti.hardware.capabilityconfigstore@1.0-service
    override
    class hal
    user system
    group system
    disabled

service vendor.imsdatadaemon /vendor/bin/imsdatadaemon
    override
    class main
    user radio
    socket ims_datad stream 0660 system radio
    group radio wifi inet vendor_qti_diag log
    disabled

on property:init.svc.cnss_diag=running
    stop cnss_diag

on property:init.svc.tcpdump=running
    stop tcpdump

on property:init.svc.vendor.ipacm-diag=running
    stop vendor.ipacm-diag
